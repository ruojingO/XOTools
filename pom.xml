			 <plugin>
        <groupId>org.owasp</groupId>
        <artifactId>dependency-check-maven</artifactId>
        <version>12.1.1</version> <executions>
          <execution>
            <goals>
              <goal>check</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <failBuildOnCVSS>7</failBuildOnCVSS>
          <enableExperimental>false</enableExperimental>
          <nvdDatafeedUrl>https://nvd.nist.gov/feeds/json/cve/1.1/nvdcve-1.1-%d{yyyy}.json.gz</nvdDatafeedUrl>
          <nvdValidForHours>72</nvdValidForHours>
		    <disableRetireJS>true</disableRetireJS>
          <outputDirectory>${project.build.directory}/dependency-check-reports</outputDirectory>
          <formats>
            <format>json</format>
            <format>html</format>
          </formats>
          </configuration>
      </plugin>
	  
	  <plugin>
  <groupId>org.codehaus.mojo</groupId>
  <artifactId>versions-maven-plugin</artifactId>
  <version>2.16.2</version>
  
  <configuration>
	  <allowSnapshots>false</allowSnapshots>
	  <allowMajorUpdates>false</allowMajorUpdates>
	  <includeNextSnapshots>false</includeNextSnapshots>
	  <includeNextReleases>true</includeNextReleases>
	  <includeNextDevelopmentVersions>false</includeNextDevelopmentVersions>
</configuration>
</plugin>

